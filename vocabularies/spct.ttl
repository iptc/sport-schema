spct:sport
	rdf:type skos:Concept;
	skos:prefLabel	"Sport"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:league
	rdf:type skos:Concept;
	skos:prefLabel	"League"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:season
	rdf:type skos:Concept;
	skos:prefLabel	"Season"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:season-type
	rdf:type skos:Concept;
	skos:prefLabel	"season-type"@en-GB;
	skos:definition	"Which segment of the season: pre-season, post-season, regular, etc."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:season-regular
	rdf:type skos:Concept;
	skos:prefLabel	"Regular Season"@en-GB;
	skos:definition	"The phase of a season during which competitors vie for placement in a playoff phase."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:post-season
	rdf:type skos:Concept;
	skos:prefLabel	"Post Season"@en-GB;
	skos:definition	"The phase of a season after the regular season during which competitors vie for the league championship."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:season-exhibition
	rdf:type skos:Concept;
	skos:prefLabel	"Pre Season"@en-GB;
	skos:definition	"The phase of a season during which exhibition matches are played."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:competition
	rdf:type skos:Concept;
	skos:prefLabel	"Competition"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:recurring-competition
	rdf:type skos:Concept;
	skos:prefLabel	"Recurring competition"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:tournament
	rdf:type skos:Concept;
	skos:prefLabel	"Tournament"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:conference
	rdf:type skos:Concept;
	skos:prefLabel	"Conference"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:division
	rdf:type skos:Concept;
	skos:prefLabel	"Division"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:news
	rdf:type skos:Concept;
	skos:prefLabel	"News"@en-GB;
	skos:definition	"A category of sports documents: non-event-specific news reports for player, team, league, etc."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:standings
	rdf:type skos:Concept;
	skos:prefLabel	"Standings"@en-GB;
	skos:definition	"A category of sports documents: often also called tables"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:schedules
	rdf:type skos:Concept;
	skos:prefLabel	"Schedules"@en-GB;
	skos:definition	"A category of sports documents."@en-GB;
