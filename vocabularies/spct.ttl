spct:statistics
	rdf:type skos:Concept;
	skos:prefLabel	"Statistics"@en-GB;
	skos:definition	"A category of sports documents: for non-event-specific statistical reports"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:event-summary
	rdf:type skos:Concept;
	skos:prefLabel	"Event Summary"@en-GB;
	skos:definition	"A category of sports documents: for event-specific reports whether prose (i.e. previews and reviews) or statistical (i.e.  boxscores))"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spct/>.

spct:event-action
	rdf:type skos:Concept;
	skos:prefLabel	"Event Action"@en-GB;
