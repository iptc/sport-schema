sport:OfficialParticipationShape rdf:type sh:NodeShape  ;
  a sh:NodeShape ;
  sh:targetClass sport:OfficialParticipation ;
  sh:closed true;
  sh:ignoredProperties ( rdf:type rdfs:label ) ;
  sh:property [
    sh:path sport:participationBy ;
    sh:class sport:Official
