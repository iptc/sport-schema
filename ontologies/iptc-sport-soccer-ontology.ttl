@prefix iptcs: <http://www.iptc.org/ontologies/Sport/ontologies/sport/> . 
@prefix iptcsc: <http://www.iptc.org/ontologies/Sport/ontologies/soccer/> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .

#######  Classes ##### 	



iptcsc:SoccerTeamPerformance rdf:type owl:Class ;
	rdfs:label "SoccerTeamPerformance"@en ;
	rdfs:subClass iptcs:TeamParticipation ;
	rdfs:comment "A soccer teams performance"@en ;
	rdf:isDefinedBy <http://www.iptc.org/ontologies/Sport/ontologies/soccer/> .

iptcsc:SoccerIndividualPerformance rdf:type owl:Class ;
	rdfs:label "SoccerIndividualPerformance"@en ;
	rdfs:subClass iptcs:IndividualParticipation ;
	rdfs:comment "A soccer players performance"@en ;
	rdf:isDefinedBy <http://www.iptc.org/ontologies/Sport/ontologies/soccer/> .	



