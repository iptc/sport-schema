@prefix sport: <http://www.iptc.org/ontologies/Sport/> . 
@prefix soccer: <http://www.iptc.org/ontologies/Sport/Soccer/> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .

#######  Classes ##### 	



soccer:SoccerTeamPerformance rdf:type owl:Class ;
	rdfs:label "SoccerTeamPerformance"@en ;
	rdfs:subClass sport:TeamParticipation ;
	rdfs:comment "A soccer teams performance"@en ;
	rdf:isDefinedBy <http://www.iptc.org/ontologies/Sport/Soccer/> .

soccer:SoccerIndividualPerformance rdf:type owl:Class ;
	rdfs:label "SoccerIndividualPerformance"@en ;
	rdfs:subClass sport:IndividualParticipation ;
	rdfs:comment "A soccer players performance"@en ;
	rdf:isDefinedBy <http://www.iptc.org/ontologies/Sport/Soccer/> .	



