
@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix skos:	<http://www.w3.org/2004/02/skos/core#>.
@prefix owl:	<http://www.w3.org/2002/07/owl#>.
@prefix spcompetitionscope:	<http://cv.iptc.org/newscodes/spcompetitionscope/>
		
<http://cv.iptc.org/newscodes/spcompetitionscope/> 
	rdf:type skos:ConceptScheme;
	skos:HasTopConcept		spcompetitionscope:events-all;
	skos:HasTopConcept		spcompetitionscope:events-home;
	skos:HasTopConcept		spcompetitionscope:events-away;
	skos:HasTopConcept		spcompetitionscope:division;
	skos:HasTopConcept		spcompetitionscope:division-opposing;
	skos:HasTopConcept		spcompetitionscope:conference;
	skos:HasTopConcept		spcompetitionscope:conference-opposing;
	skos:HasTopConcept		spcompetitionscope:round;
	skos:HasTopConcept		spcompetitionscope:team-opposing;
	skos:HasTopConcept		spcompetitionscope:player-opposing;
	skos:HasTopConcept		spcompetitionscope:tournament;
	skos:HasTopConcept		spcompetitionscope:league;
	skos:HasTopConcept		spcompetitionscope:top-25.

spcompetitionscope:events-all
	rdf:type skos:Concept;
	skos:prefLabel	"events-all"@en-GB;
	skos:definition	"Every event played, including those played against teams in the same division or conference. The default option."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:events-home
	rdf:type skos:Concept;
	skos:prefLabel	"events-home"@en-GB;
	skos:definition	"Every event played at the team's home location."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:events-away
	rdf:type skos:Concept;
	skos:prefLabel	"events-away"@en-GB;
	skos:definition	"Every event played away from the team's home location."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:division
	rdf:type skos:Concept;
	skos:prefLabel	"division"@en-GB;
	skos:definition	"Events played within a league's division."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:division-opposing
	rdf:type skos:Concept;
	skos:prefLabel	"division-opposing"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:conference
	rdf:type skos:Concept;
	skos:prefLabel	"conference"@en-GB;
	skos:definition	"Events played within a league's conference."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:conference-opposing
	rdf:type skos:Concept;
	skos:prefLabel	"conference-opposing"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:round
	rdf:type skos:Concept;
	skos:prefLabel	"round"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:team-opposing
	rdf:type skos:Concept;
	skos:prefLabel	"team-opposing"@en-GB;
	skos:definition	"Events against a specific team."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:player-opposing
	rdf:type skos:Concept;
	skos:prefLabel	"player-opposing"@en-GB;
	skos:definition	"Events against a specific player."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:tournament
	rdf:type skos:Concept;
	skos:prefLabel	"tournament"@en-GB;
	skos:definition	"Events from a specific tournament."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:league
	rdf:type skos:Concept;
	skos:prefLabel	"league"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.

spcompetitionscope:top-25
	rdf:type skos:Concept;
	skos:prefLabel	"top-25"@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/spcompetitionscope/>.
