
@prefix rdf:	<http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix skos:	<http://www.w3.org/2004/02/skos/core#>.
@prefix owl:	<http://www.w3.org/2002/07/owl#>.
@prefix speventstatus:	<http://cv.iptc.org/newscodes/speventstatus/>
		
<http://cv.iptc.org/newscodes/speventstatus/> 
	rdf:type skos:ConceptScheme;
	skos:hasTopConcept		speventstatus:pre-event;
	skos:hasTopConcept		speventstatus:mid-event;
	skos:hasTopConcept		speventstatus:post-event;
	skos:hasTopConcept		speventstatus:postponed;
	skos:hasTopConcept		speventstatus:suspended;
	skos:hasTopConcept		speventstatus:halted;
	skos:hasTopConcept		speventstatus:forfeited;
	skos:hasTopConcept		speventstatus:rescheduled;
	skos:hasTopConcept		speventstatus:delayed;
	skos:hasTopConcept		speventstatus:canceled;
	skos:hasTopConcept		speventstatus:intermission;
	skos:hasTopConcept		speventstatus:if-necessary;
	skos:hasTopConcept		speventstatus:discarded.

speventstatus:pre-event
	rdf:type skos:Concept;
	skos:prefLabel	"pre-event"@en-GB;
	skos:definition	"Any time before an event begins."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:mid-event
	rdf:type skos:Concept;
	skos:prefLabel	"mid-event"@en-GB;
	skos:definition	"Any time while an event is in-play."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:post-event
	rdf:type skos:Concept;
	skos:prefLabel	"post-event"@en-GB;
	skos:definition	"Any time after an event is finished."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:postponed
	rdf:type skos:Concept;
	skos:prefLabel	"postponed"@en-GB;
	skos:definition	"The putting off of an event until a later date, before the event has begun, for whatever reason."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:suspended
	rdf:type skos:Concept;
	skos:prefLabel	"suspended"@en-GB;
	skos:definition	"An event is stopped with the intention of resumption at a later date."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:halted
	rdf:type skos:Concept;
	skos:prefLabel	"halted"@en-GB;
	skos:definition	"An event is stopped and not resumed or ended."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:forfeited
	rdf:type skos:Concept;
	skos:prefLabel	"forfeited"@en-GB;
	skos:definition	"A team declines or is unable to play and loses by default. The opponent is awarded the win."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:rescheduled
	rdf:type skos:Concept;
	skos:prefLabel	"rescheduled"@en-GB;
	skos:definition	"An event is given a new time and date."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:delayed
	rdf:type skos:Concept;
	skos:prefLabel	"delayed"@en-GB;
	skos:definition	"An event is held up, for example by inclement weather. Can happen before or during an event."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:canceled
	rdf:type skos:Concept;
	skos:prefLabel	"canceled"@en-GB;
	skos:definition	"An event is never played and not re-scheduled."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:intermission
	rdf:type skos:Concept;
	skos:prefLabel	"intermission"@en-GB;
	skos:definition	"A planned or scheduled break in play after which play resumes."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:if-necessary
	rdf:type skos:Concept;
	skos:prefLabel	"if-necessary"@en-GB;
	skos:definition	"An event whose commencement is dependant upon the outcome of prior events."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.

speventstatus:discarded
	rdf:type skos:Concept;
	skos:prefLabel	"discarded"@en-GB;
	skos:definition	"A cancelled event because the event was unnecessary."@en-GB;
	skos:inScheme <http://cv.iptc.org/newscodes/speventstatus/>.
